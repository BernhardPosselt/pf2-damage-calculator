{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "lib": [
      "dom",
      "ES2020",
      "ESNext"
    ],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "typeRoots": [
      "src/typings",
      "node_modules/@types"
    ]
  }
}
